            <MudMenuItem Href="#" Label="Transporte" />
            <MudMenuItem Href="#" Label="Item Eletrico" />
        </MudMenu>
    </MudStack>

    <MudSpacer />

    <MudIconButton Href="/" Icon="@Icons.Material.Filled.Home" Color="Color.Inherit" />
